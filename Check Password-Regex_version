import re 

def main():
	passwd = 'Geek12@'
	reg = "^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)(?=.*[@$!%*#?&])[A-Za-z\d@#$%&*?]{6,20}$"
	pat = re.compile(reg)
	mat = re.search(pat, passwd)
	if mat:
		print("Password is valid.")
	else:
		print("Password is invalid !!")
#Driver code
if __name__ == '__main__':
	main()
